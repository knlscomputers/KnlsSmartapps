<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Signup"
    android:orientation="vertical"
    android:background="#fff"
    android:padding="20dp"
    >

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:fontFamily="@font/bungee"
        android:text="KNLS ONLINE SERVICES"
        android:textColor="#000"
        android:textSize="20sp"
        android:layout_gravity="center"
        android:textAlignment="center"
        android:transitionName="logo_text" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:fontFamily="@font/bungee"
        android:text="PF Ekyc and Claims "
        android:layout_gravity="center"
        android:textColor="#000"
        android:textSize="20sp"
        android:textAlignment="center"
        android:layout_margin="5dp"
        android:transitionName="logo_text" />


    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:text="Please Enter the Following Details"
        android:textSize="20sp"
        android:textAlignment="center"
         android:transitionName="logo_desc"
        />


    <EditText
        android:id="@+id/pfUan"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:textSize="20sp"
        android:fontFamily="@font/antic"
        android:hint="Enter Your UAN No.."
        android:inputType="text"
        android:textColorHint="#A5A5A5"/>


    <EditText
        android:id="@+id/pfCellno"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:textSize="20sp"
        android:fontFamily="@font/antic"
        android:hint="Cell No."
        android:inputType="text"
        android:textColorHint="#A5A5A5"/>




    <EditText
        android:id="@+id/pfName"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:textSize="20sp"
        android:fontFamily="@font/antic"
        android:hint="Name as per PF Records"
        android:inputType="text"
        android:textColorHint="#A5A5A5"/>


    <EditText
        android:id="@+id/pfDob"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:textSize="20sp"
        android:fontFamily="@font/antic"
        android:hint="Date of Birth"
        android:inputType="text"
        android:textColorHint="#A5A5A5"
 />

    <EditText
        android:id="@+id/pfAadhar"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:textSize="20sp"
        android:fontFamily="@font/antic"
        android:hint="AADHAR"
        android:inputType="text"
        android:textColorHint="#A5A5A5"
 />

    <EditText
        android:id="@+id/pfBank"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:textSize="20sp"
        android:fontFamily="@font/antic"
        android:hint="Bank A/c no."
        android:inputType="text"
        android:textColorHint="#A5A5A5"
     />

    <EditText
        android:id="@+id/pfIfsccode"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:textSize="20sp"
        android:fontFamily="@font/antic"
        android:hint="BANK IFSC"
        android:inputType="text"
        android:textColorHint="#A5A5A5"
 />

    <EditText
        android:id="@+id/pfPAN"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:textSize="20sp"
        android:fontFamily="@font/antic"
        android:hint="PAN No."
        android:inputType="text"
        android:textColorHint="#A5A5A5"
        />


    <Button
        android:id="@+id/pf_Submtbtn"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:background="#000"
        android:text="Submit"
        android:textColor="#fff"
        android:transitionName="button_tran" />


</LinearLayout>

